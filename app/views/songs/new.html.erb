<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <%= f.label :notes %>
  <%= f.text_field :notes, id: "song_notes_1", multiple: true, value: '' %>
  <%= f.label :notes %>
  <%= f.text_field :notes, id: "song_notes_2", multiple: true, value: '' %>
  <%= f.submit %>
<% end %>



